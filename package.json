{
  "name": "moonspell-themes",
  "displayName": "Moonspell Themes",
  "description": "Ethereal",
  "license": "MIT",
  "version": "0.0.4",
  "publisher": "oodri",
  "repository": {
    "type": "git",
    "url": "https://github.com/ElvannAbendroth/moonspell-themes"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "icon": "icon.png",
  "categories": [
    "Themes"
  ],
  "scripts": {
    "build": "vsce package -o ./.build",
    "publish": "vsce publish --skip-duplicate",
    "dev": "nodemon --watch themes --watch src --ext ts --exec \"ts-node ./index.ts\""
  },
  "contributes": {
    "themes": [
      {
        "label": "Moonspell - Amethyst",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-amethyst.json"
      },
      {
        "label": "Moonspell - Northern Lights",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-northern-lights.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "ts-node": "^10.9.2",
    "vsce": "^2.15.0"
  },
  "cspell": {
    "version": "0.2",
    "ignorePaths": [],
    "dictionaryDefinitions": [],
    "dictionaries": [],
    "words": [
      "Arstotzka",
      "Catppuccin",
      "Gruvbox",
      "Kanagawa",
      "Moonspell",
      "Nord"
    ],
    "ignoreWords": [],
    "import": []
  },
  "keyword": [
    "Theme",
    "Oodri Theme",
    "Aurora",
    "Dark Theme",
    "Black Theme",
    "Arstotzka Theme",
    "City Lights Theme",
    "Duotone Theme",
    "Drawbridge Theme",
    "Gruvbox Theme",
    "Night Owl Theme",
    "Nord Theme",
    "Papers, Please Theme",
    "Radical Theme",
    "In Bed By 7pm",
    "Kanagawa Theme",
    "Catppuccin Theme",
    "Moonspell Theme"
  ]
}
